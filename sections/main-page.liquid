{{ 'section-main-page.css' | asset_url | stylesheet_tag }}

<div class="page-width page-width--narrow page-padded">
  <h1 class="title title--primary h2">
    {{ page.title | escape }}
  </h1>
  <div class="rte">
    {{ page.content }}
  </div>
</div>

{% schema %}
{
  "name": "t:sections.main-page.name",
  "tag": "section",
  "class": "section",
  "settings": []
}
{% endschema %}
